<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ua.lviv.iot.ted.mobiledev">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ua.lviv.iot.ted.mobiledev.AddGameActivity"></activity>
        <activity android:name="ua.lviv.iot.ted.mobiledev.GameActivity" />
        <activity android:name="ua.lviv.iot.ted.mobiledev.SignUpActivity" />
        <activity android:name="ua.lviv.iot.ted.mobiledev.MainActivity" />
        <activity android:name="ua.lviv.iot.ted.mobiledev.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>